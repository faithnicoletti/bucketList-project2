<%- include('../partials/header') %>
<div class="pricing-header p-3 pb-md-4 mx-auto text-center">
    <h2 class="display-5 fw-normal text-body-emphasis shadow-lg m-10">Fitness/Health: Completed</h2>
  </div>
  <div class="row row-cols-3 row-cols-md-3 mb-3 text-center justify-content-center align-items-center" style="margin: 0 auto;" >
    <div class="col justify-content-center">
      <div class="card mb-6 mt-6 rounded-5 shadow-lg justify-content-center">
    <ul class="list-unstyled mt-4 mb-4">
      <% fitness.forEach((fitness) => { %>
        <li>
          <a href="/dreams/fitnessCompleted/<%= fitness.id %>">
            <button type="button" class="w-50 m-1 btn btn-md btn-outline-dark"><%= fitness.name %></button>
          </a>
        </li>
      <% }) %>
    </ul>
  </div>  
