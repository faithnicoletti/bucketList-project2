<%- include('../partials/header') %>
<a href="/dreams/index-completed">
  <button class="w-20 m-2 btn btn-sm btn-outline-dark">Back</button>
</a>
<div class="pricing-header p-3 pb-md-4 mx-auto text-center">
  <h1 class="display-4 fw-normal text-body-emphasis shadow-sm m-5">Cultural Explorations: Completed</h1>
  </div>
<div class="row row-cols-2 row-cols-md-3 mb-3 text-center justify-content-center align-items-center">
  <div class="col">
    <div class="card mb-4 rounded-3 shadow-sm">
      <div class="card-header py-3">
        <h4 class="my-0 fw-normal">Completed Goals Listed:</h4>
      </div>
      <div class="card-body">
        <ul class="list-unstyled mt-4 mb-4">
          <% taste.forEach((taste) => { %>
            <li>
              <a href="/dreams/tasteCompleted/<%= taste.id %>">
                <button type="button" class="w-50 m-1 btn btn-md btn-outline-dark"><%= taste.name %></button>
              </a>
            </li>
          <% }) %>
        </ul>
      </div>
    </div>         
  </div>
<%- include('../partials/footer') %>